{
  "version": 1,
  "name": "reddix",
  "alias": "reddix.xyz",
  "builds": [
    {
      "src": "package.json",
      "use": "@now/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*\\.(js|ts|jpg|json|css|ico|png)$)",
      "dest": "/$1",
      "headers": {"cache-control": "public,max-age=31536000,immutable"}
    }
  ]
}